<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.collegeManagement">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ksit_logo_edited"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ksit_logo_edited"
        android:supportsRtl="true"
        android:theme="@style/Theme.CollegeManagement"
        tools:ignore="LockedOrientationActivity">
        <activity
            android:name="com.example.collegeManagement.AddCollection"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.collegeManagement.StudentPage"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.example.collegeManagement.UpdateDocument"
            android:configChanges="orientation"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.collegeManagement.ViewDocuments"
            android:configChanges="orientation"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.collegeManagement.TeacherPage"
            android:configChanges="orientation"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.collegeManagement.LoginActivity"
            android:configChanges="orientation"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.collegeManagement.AddUser"
            android:configChanges="orientation"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.collegeManagement.AdminPage"
            android:configChanges="orientation"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.collegeManagement.MainActivity"
            android:configChanges="orientation"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>